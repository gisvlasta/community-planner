<body>
  
  {{!--Wrap all page content here--}}
  <div id="wrap" class="container-fluid">
    
    <div id="header">
      <div class="container">
        
        {{!--Fixed navbar--}}
        <div class="navbar navbar-default navbar-fixed-top" role="navigation">
          <div id="navbar-container" class="container">
            <div class="navbar-header">
              <button type="button" data-toggle="collapse" data-target=".navbar-collapse" class="navbar-toggle">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
              </button>
              <a href="/community-planner/" class="navbar-brand">Community Planner</a>
            </div>
            <div class="collapse navbar-collapse">
              <ul class="nav navbar-nav navbar-left">
                {{#each navLinks}}
                  {{#if nestedNavLinks}}
                    <li class="dropdown">
                      <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="true">Maps <span class="caret"></span></a>
                      <ul class="dropdown-menu">
                        {{#each nestedNavLinks}}
                          <li {{#ifeq ../section key}} class="active" {{else}} {{/ifeq}}>
                            <a href="{{ href }}">{{ label }}</a>
                          </li>
                        {{/each}}
                      </ul>      
                    </li>
                  {{else}}
                    <li {{#ifeq ../section key}} class="active" {{else}} {{/ifeq}}>
                      <a href="{{ href }}">{{ label }}</a>
                    </li>
                  {{/if}}
                {{/each}}
              </ul>
              <ul class="nav navbar-nav navbar-right">
                {{#if user}}
                <li><a href="/community-planner/keystone/signout">Sign Out</a></li>
                {{else}}
                <li><a href="/community-planner/keystone/signin">Sign In</a></li>
                {{/if}}
              </ul>
            </div>
          </div>
        </div>
        
      </div>
    </div>
    
    {{!--Begin page content--}}
    <div class="center-container">
      <div class="center-row">
        <div id="body" class="container">
          {{!--
            Flash messages allow you to display once-off status messages to users, e.g. form
            validation errors, success messages, etc
          --}}
          {{#if messages}}
          <div id="flash-messages" class="container">
            {{#if messages.info}}
            <div class="alert alert-info">
              {{{flashMessages messages.info}}}
            </div>
            {{/if}}
            {{#if messages.success}}
            <div class="alert alert-success">
              {{{flashMessages messages.success}}}
            </div>
            {{/if}}
            {{#if messages.warning}}
            <div class="alert alert-warning">
              {{{flashMessages messages.warning}}}
            </div>
            {{/if}}
            {{#if messages.error}}
            <div class="alert alert-danger">
              {{{flashMessages messages.error}}}
            </div>
            {{/if}}
          </div>
          {{/if}}
          {{!--The content block should contain the body of your template's content--}}
          {{{body}}}
        </div>
      </div>
    </div>
    
  </div>
  
  <div id="footer">
